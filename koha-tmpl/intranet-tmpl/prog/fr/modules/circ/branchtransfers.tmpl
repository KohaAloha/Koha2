                                    <input type="hidden" name="tb-<!-- TMPL_VAR Name="counter" -->" value="<!-- TMPL_VAR Name="tobrcd" -->" />
                                <!-- /TMPL_LOOP -->
                                <input type="submit" value="Ignorer">
                            </form>
                        </td>
                </tr>
    </table>

<!-- TMPL_ELSE -->

        <h4>Messages</h4>
		<ul>
                <!-- TMPL_IF Name="reqmessage" -->
