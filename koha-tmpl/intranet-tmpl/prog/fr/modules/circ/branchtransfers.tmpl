        <!-- /TMPL_LOOP -->
        <input type="submit" value="Transférer">
    </form>
        
    <!-- TMPL_IF Name="trsfitemloop" -->
        <table>
            <caption>Exemplaires transférés</caption>
            <tr>
                <th>Code à barres</th>
