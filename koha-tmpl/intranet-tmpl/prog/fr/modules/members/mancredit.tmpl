<h2>Crédit manuel</h2>
<form action="/cgi-bin/koha/members/mancredit.pl" method="post">
<input type="hidden" name="borrowernumber" value="<!-- TMPL_VAR Name="borrowernumber" -->" />

<table>
	<tr><th scope="row">Numéro d'adhérent</th><td><!-- TMPL_VAR Name="borrowernumber" --></td></tr>
	<tr><th scope="row">Type de crédit</th><td><select name="type">
<option value="C">Crédit</option>
<option value="F">Dette remise</option>
</select></td></tr>
	<tr><th scope="row">Numéro d'exemplaire</th><td><input type="text" name="itemnum" /></td></tr>
	<tr><th scope="row">Description</th><td><input type="text" name="desc" size="50" /></td></tr>
	<tr><th scope="row">Montant</th><td><input type="text" name="amount" /> €</td></tr>
</table>

<input type="submit" name="add" value="Ajouter  crédit">
</form>

<form action="/cgi-bin/koha/members/boraccount.pl" method="get"><input type="hidden" value="<!-- TMPL_VAR Name="borrowernumber" -->" name="borrowernumber" /><input type="submit" value="Annuler"></form>



