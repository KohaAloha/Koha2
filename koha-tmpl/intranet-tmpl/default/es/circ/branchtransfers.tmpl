<TMPL_INCLUDE NAME="circulation-top.inc">

<table align=right><tr><td>
<a href=circulation.pl>
<img src="<TMPL_VAR name="themelang">/images/button-issues.gif" width="99" height="42" border="0" alt="Pr&oacute;ximo Usuario"></a> &nbsp
<a href=returns.pl>
<img src="<TMPL_VAR name="themelang">/images/button-returns.gif" width="110" height="42" border="0" alt="Devoluciones"></a>
</td></tr></table>

<FONT SIZE=6><em>Circulaci&oacute;n: Transferencias</em></FONT><br>
<b>Branch:</b> <TMPL_VAR Name="genbrname"> &nbsp
<b>Printer:</b> <TMPL_VAR Name="genprname"><br>
<a href=selectbranchprinter.pl>Cambiar Configuraci&oacute;n</a>
<input type=hidden name=branch value=<TMPL_VAR Name="branch">>
<input type=hidden name=printer value=<TMPL_VAR Name="printer">>
<p>

<TMPL_IF Name="found">
	<table border=1 cellpadding=5 cellspacing=0 bgcolor='#dddddd'>
	<tr><th background="<TMPL_VAR name="themelang">/images/background-mem.gif"><font>Reserva Encontrada</font></th></tr>
	<tr><td> <font color='red' size='+2'>
		<TMPL_IF Name="reserved">Reservado:</font> reserva encontrada para <TMPL_VAR Name="name"> (<a href=/cgi-bin/koha/moremember.pl?bornum=<TMPL_VAR Name="bornum"> onClick="openWindow(this,'Member', 480, 640)"><TMPL_VAR Name="borcnum"></a>).</TMPL_IF>
		<TMPL_IF Name="waiting"> Item marked Waiting:<br>
    El Item esta esperando en <TMPL_VAR Name="branchname"> por <TMPL_VAR Name="name"> (<a href=/cgi-bin/koha/moremember.pl?bornum=<TMPL_VAR Name="bornum"> onClick="openWindow(this,'Member', 480, 640)"><TMPL_VAR Name="borcnum"></a>).<br></TMPL_IF>
		<table border=1 cellpadding=5 cellspacing=0>
		<tr><td>
			<TMPL_IF Name="reserved">Marcar reserva como esperando y tranferir el libro a <b><TMPL_VAR Name="branchname"> </b>: </TMPL_IF>
			<TMPL_IF Name="waiting">Cancelar reserva e intentar transferencia: </TMPL_IF>
		</td>
		<td>
			<form method=post action='branchtransfers.pl'>
				<TMPL_LOOP Name="trsfitemloop">
					<input type=hidden name=bc-><TMPL_VAR Name="counter"> value=<TMPL_VAR Name="barcode">>
					<input type=hidden name=fb-><TMPL_VAR Name="counter"> value=<TMPL_VAR Name="frombrcd">>
					<input type=hidden name=tb-><TMPL_VAR Name="counter"> value=<TMPL_VAR Name="tobrcd">>
				</TMPL_LOOP>
				<input type=hidden name=itemnumber value=<TMPL_VAR Name="itemnumber">>
				<input type=hidden name=borrowernumber value=<TMPL_VAR Name="bornum">>
				<TMPL_IF Name="waiting"><input type=hidden name=barcode value=<TMPL_VAR Name="barcode">>
					<input type=hidden name=request value='KillWaiting'>
					<input type=submit value="Cancelar">
				</TMPL_IF>
				<TMPL_IF Name="reserved"><input type=hidden name=request value='SetWaiting'>
					<input type=submit value="Esperando"></TMPL_IF>
			</form>
		</td></tr>
		<TMPL_IF Name="reserved"><tr><td>Cancelar reserva e intentar transferencia: </td>
			<td>
			<form method=post action='branchtransfers.pl'>

			<TMPL_LOOP Name="trsfitemloop">
				<input type=hidden name=bc-><TMPL_VAR Name="counter"> value=<TMPL_VAR Name="barcode">>
				<input type=hidden name=fb-><TMPL_VAR Name="counter"> value=<TMPL_VAR Name="frombrcd">>
				<input type=hidden name=tb-><TMPL_VAR Name="counter"> value=<TMPL_VAR Name="tobrcd">>
			</TMPL_LOOP>

			<input type=hidden name=biblionumber value=<TMPL_VAR Name="biblionumber">>
			<input type=hidden name=borrowernumber value=<TMPL_VAR Name="bornum">>
			<input type=hidden name=tobranchcd value=<TMPL_VAR Name="tobranchcd">>
			<input type=hidden name=barcode value=<TMPL_VAR Name="barcode">>
			<input type=hidden name=request value='KillReserved'>
			<input type=submit value="Cancelar">
			</form>
		</TMPL_IF>
		</table></td></tr>
		<tr><td>Ignorar y volver a transferencias: </td>
			<td>
			<form method=post action='branchtransfers.pl'>
			<input type=hidden name=tobranchcd value=<TMPL_VAR Name="tobranchcd">>
			<input type=hidden name=barcode value=0>
			<TMPL_LOOP Name="trsfitemloop">
				<input type=hidden name=bc-><TMPL_VAR Name="counter"> value=<TMPL_VAR Name="barcode">>
				<input type=hidden name=fb-><TMPL_VAR Name="counter"> value=<TMPL_VAR Name="frombrcd">>
				<input type=hidden name=tb-><TMPL_VAR Name="counter"> value=<TMPL_VAR Name="tobrcd">>
			</TMPL_LOOP>
			<input type=submit value="Ignorar">
			</form>
			</td>
		</tr></table>


<TMPL_ELSE>
	<table border=1 cellpadding=5 cellspacing=0 bgcolor='#dddddd'>
	<tr>
    <th background="<TMPL_VAR name="themelang">/images/background-mem.gif"><font>Mensajes</font></th>
  </tr>
	<tr><td>
	<TMPL_IF Name="reqmessage">
		<TMPL_IF Name="cancelled">
			Reserva Cancelada<br>
		</TMPL_IF>
		<TMPL_IF Name="setwaiting">
			El Item deber&iacute;a estar ahora esperando en la Biblioteca: <TMPL_VAR Name="reqbrchname"><br>
		</TMPL_IF>
	</TMPL_IF>
	<TMPL_LOOP Name="errmsgloop">
		<font color='red' size='+2'>
		<TMPL_IF Name="errbadcode">
			No hay un Item con c&oacute;digo: <TMPL_VAR Name="msg">
		</TMPL_IF>
		<TMPL_IF Name="errispermanent">
			Por favor regrese el item a la biblioteca de origen: <TMPL_VAR Name="msg">
		</TMPL_IF>
		<TMPL_IF Name="errdesteqholding">
			El Item ya se encuentran en la biblioteca de destino.
		</TMPL_IF>
		<TMPL_IF Name="errwasreturned">
			El Item estaba prestado a <a href=/cgi-bin/koha/moremember.pl?bornum=<TMPL_VAR Name="bornum">
onClick="openWindow(this,'Member', 480, 640)"><TMPL_VAR Name="borcnum"></a>
<TMPL_VAR Name="name"> y ha sido devuelto.
		</TMPL_IF>
		</font>
		<BR>
	</TMPL_LOOP>
	</td></tr></table>
	<img src="<TMPL_VAR name="themelang">/images/holder.gif" width=24 height=24>
	<form method=post action=/cgi-bin/koha/circ/branchtransfers.pl>
	<table border=1 cellpadding=5 cellspacing=0 align=left>
	<tr><td colspan=2 align=center background="<TMPL_VAR name="themelang">/images/background-mem.gif">
	<font color=black><b>Seleccione una Biblioteca</b></font></td></tr>
	<tr><td>Biblioteca de Destino:</td>
		<td><select name=tobranchcd>
			<TMPL_LOOP Name="branchoptionloop">
				<option value=<TMPL_VAR Name="code"> <TMPL_VAR Name="selected">><TMPL_VAR Name="name"> <br>
			</TMPL_LOOP>
		</select></td></tr></table>

	<img src="<TMPL_VAR name="themelang">/images/holder.gif" width=24 height=50 align=left>

	<table border=1 cellpadding=5 cellspacing=0 ><tr>
	<td colspan=2 align=center background="<TMPL_VAR name="themelang">/images/background-mem.gif">
	<font color=black><b>Ingrese c&oacute;digo del Libro</b></font></td></tr>
	<tr><td>C&oacute;digo del Item:</td><td><input name=barcode size=10></td></tr>
	</table>
	<input type=hidden name=tobranchcd value=<TMPL_VAR Name="tobrancd">>

	<TMPL_LOOP Name="trsfitemloop">
		<input type=hidden name=bc-><TMPL_VAR Name="counter"> value=<TMPL_VAR Name="barcode">>
		<input type=hidden name=fb-><TMPL_VAR Name="counter"> value=<TMPL_VAR Name="frombrcd">>
		<input type=hidden name=tb-><TMPL_VAR Name="counter"> value=<TMPL_VAR Name="tobrcd">>
	</TMPL_LOOP>

	</form>
	<TMPL_IF Name="transferred">
		<p>
		<table border=1 cellpadding=5 cellspacing=0 bgcolor=#dddddd>
		<tr><th colspan=6  background="<TMPL_VAR name="themelang">/images/background-mem.gif"><font color=black>Transfered Items</font></th></tr>
		<tr><th>C&oacute;digo</th><th>T&iacute;tulo</th><th>Autor</th><th>Type</th><th>Desde</th><th>Hasta</th></tr>
		<TMPL_LOOP Name="trsfitemloop">
			<tr><td bgcolor=<TMPL_VAR Name="color"> align=center>
			<a href=/cgi-bin/koha/detail.pl?bib=<TMPL_VAR Name="biblionumber">
			&type=intra onClick=\"openWindow(this, 'Item', 480, 640)\"><TMPL_VAR Name="barcode"></a></td>
			<td bgcolor=<TMPL_VAR Name="color">><TMPL_VAR Name="title"></td>
			<td bgcolor=<TMPL_VAR Name="color">><TMPL_VAR Name="author"></td>
			<td bgcolor=<TMPL_VAR Name="color"> align=center><TMPL_VAR Name="itemtype"></td>
			<td bgcolor=<TMPL_VAR Name="color"> align=center><TMPL_VAR Name="frbrname"></td>
			<td bgcolor=<TMPL_VAR Name="color"> align=center><TMPL_VAR Name="tobrname"></td>
			</tr>
		</TMPL_LOOP>
		</table>
	</TMPL_IF>
</TMPL_IF>


<TMPL_INCLUDE Name="circulation-bottom.inc">
