<!-- TMPL_INCLUDE NAME="parameters-top.inc" -->

<table width="100%" cellpadding=5 cellspacing=5>
	<tr background="<!-- TMPL_VAR NAME='themelang' -->/images/background-mem.gif">
		<td><b>測試</b></td>
		<td><b>結果</b></td>
	</tr>
<!------------------------------------------------------------------------------------------------------------>
<!-- TMPL_IF name="itemnum" -->
	<tr>
		<td><b>圖書資料號碼</b></td>
		<td><li>The field itemnum MUST be mapped </li>
			<li>The correspounding subfield MUST be in with -1 (ignore) tab</td>
	</tr>
<!-- TMPL_ELSE -->
	<tr>
		<td>好！</td>
		<td><i>itemnum : the field itemnumber is mapped to a field in tab -1</i></td>
	</tr>
<!-- /TMPL_IF -->
<!------------------------------------------------------------------------------------------------------------>
<!-- TMPL_IF name="itemfields" -->
	<tr>
		<td><b>圖書資料相關欄位</b></td>
		<td>圖書資料的所有欄位都必須：<br/>
			<li>be mapped to the same tag,</li>
			<li>and they must all be in the 10 (items) tab</li></td>
	</tr>
<!-- TMPL_ELSE -->
	<tr>
		<td>好！</td>
		<td><i>All item fields are in the same tag and in item tab</i></td>
	</tr>
<!-- /TMPL_IF -->
<!------------------------------------------------------------------------------------------------------------>
<!-- TMPL_IF name="itemtypes_empty" -->
	<tr>
		<td><b>itemtypes table 是空白的</b></td>
		<td>itemtypes table是空白的，它必須有至少一個值</td>
	</tr>
<!-- TMPL_ELSE -->
	<tr>
		<td>好！</td>
		<td><i>itemtypes table 有至少一個值</i></td>
	</tr>
<!-- /TMPL_IF -->
<!------------------------------------------------------------------------------------------------------------>
<!-- TMPL_IF name="itemtype" -->
	<tr>
		<td><b>itemtype NOT mapped</b></td>
		<td>biblioitems.itemtype欄必須：<br/>
			<li>be mapped to a MARC subfield, </li>
			<li>對應的分欄必須設定容許值為itemtype</li></td>
	</tr>
<!-- TMPL_ELSE -->
	<tr>
		<td>好！</td>
		<td><i>biblioitems.itemtype 已設定</i></td>
	</tr>
<!-- /TMPL_IF -->
<!------------------------------------------------------------------------------------------------------------>
<!-- TMPL_IF name="branches_empty" -->
	<tr>
		<td><b>branch table 是空白的</b></td>
		<td>branch table是空白的，它必須至少有一個值！</td>
	</tr>
<!-- TMPL_ELSE -->
	<tr>
		<td>好！</td>
		<td><i>branch table 有至少一個值</i></td>
	</tr>
<!-- /TMPL_IF -->
<!------------------------------------------------------------------------------------------------------------>
<!-- TMPL_IF name="branch" -->
	<tr>
		<td><b>homebranch 沒有鏈結</b></td>
		<td>items.homebranch欄必須<br/>
			<li>be mapped to a MARC subfield,</li>
			<li>對應的分欄必須設定容許值為branches</li></td>
	</tr>
<!-- TMPL_ELSE -->
	<tr>
		<td>好！</td>
		<td><i>homebranch 已設定</i></td>
	</tr>
<!-- /TMPL_IF -->
<!------------------------------------------------------------------------------------------------------------>
<!-- TMPL_IF name="holdingbranch" -->
	<tr>
		<td><b>holdingbranch 沒有鏈結</b></td>
		<td>items.holdingbranch欄必須：<br/>
			<li>be mapped to a MARC subfield, </li>
			<li>對應的分欄必須設定容許值為branches</li></td>
	</tr>
<!-- TMPL_ELSE -->
	<tr>
		<td>好！</td>
		<td><i>holdingbranch 已設定</i></td>
	</tr>
<!-- /TMPL_IF -->
<!------------------------------------------------------------------------------------------------------------>
<!-- TMPL_IF name="biblionumber" -->
	<tr>
		<td><b>書目及書目記錄號碼</b></td>
		<td>biblio.biblionumber 及 biblioitems.biblioitemnumber 欄位必須：<br/>
			<li>be mapped to a MARC subfield,</li>
			<li>be in the SAME tag (for example : 090$a and 090$b)</li>
			<li>be in tab -1</li></td>
	</tr>
<!-- TMPL_ELSE -->
	<tr>
		<td>好！</td>
		<td><i>biblionumber 及 biblioitemnumber 已鏈結妥當</i></td>
	</tr>
<!-- /TMPL_IF -->
<!------------------------------------------------------------------------------------------------------------>
<!-- TMPL_IF name="total" -->
	<tr>
		<td><b>總計</b></td>
		<td>您的機讀格式設定有<b><!-- TMPL_VAR name="total" --></b>個錯誤。使用Koha前請先要改正它們。</td>
	</tr>
<!-- TMPL_ELSE -->
	<tr>
		<td>&nbsp;</td>
		<td><b>Configuration OK, you don't have errors in your MARC parameters table</b></td>
	</tr>
<!-- /TMPL_IF -->
</table>
