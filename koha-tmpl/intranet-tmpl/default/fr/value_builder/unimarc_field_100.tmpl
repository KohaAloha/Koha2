	function formeDate() {
		var now = new Date();
		var mois = now.getMonth();
		var jour = now.getDate();
		var annee = now.getYear();

		mois += 1;
		if (mois < 10) { mois = "0" + mois; }
		
		if (annee < 2000) { annee += 1900; }
		
		var dateString = annee + mois + jour;
		return dateString; 
	}

