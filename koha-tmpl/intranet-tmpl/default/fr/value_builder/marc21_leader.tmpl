					<option value="a">a- UCS/Unicode</option>
				<!-- /TMPL_IF -->
			</select>
		</td>
	</tr>
	<tr>
		<td>10- taille champ/indicateurs</td>
		<td>(auto-rempli)</td>
	<tr>
		<td>17- encodage</td>
		<td>
			<select name="f17" size="1">
				<!-- TMPL_IF name="f17 " -->
					<option value=" " SELECTED>niveau complet</option>
