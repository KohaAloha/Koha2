				<!-- TMPL_ELSE -->
					<option value="r">r- notice lien requise</option>
				<!-- /TMPL_IF -->
			</select>
		</td>
	</tr>
	<tr>
		<td>20-24 tailles</td>
		<td>(auto-rempli)</td>
	<tr>

	<tr><td colspan=2><input type="submit" value="OK"></td></tr>
